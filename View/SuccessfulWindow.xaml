<Window x:Class="EngineerKA_1._0.View.SuccessfulWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EngineerKA_1._0.View"
        mc:Ignorable="d"
        Title="SuccessfulWindow" Height="150" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="1" HorizontalAlignment="Center"  VerticalAlignment="Top"
                   FontSize="12" >ЗАГРУЗКА ДАННЫХ ИЗ ФАЙЛА В БАЗУ ПРОШЛА УСПЕШНО!</TextBlock>
        <TextBlock Grid.Row="2" HorizontalAlignment="Center"  VerticalAlignment="Top"
                   FontSize="8" >ДЛЯ ПРОДОЛЖЕНИЯ РАБОТЫ НАЖМИТЕ "ОК"</TextBlock>
       
        <Button Grid.Row="2" Height="20" Width="45" Content="ОК"
                HorizontalAlignment="Center" VerticalAlignment="Bottom"  Margin="10,10,10,5" 
               />
    </Grid>
</Window>

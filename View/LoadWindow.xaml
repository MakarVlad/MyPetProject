<Window x:Class="EngineerKA_1._0.View.LoadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EngineerKA_1._0.View"
        mc:Ignorable="d"
        Title="ЗАГРУЗИТЬ ДАННЫЕ ИЗ ФАЙЛА" Height="250" Width="350"
         Background="Moccasin">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/> 
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="1" HorizontalAlignment="Center"  VerticalAlignment="Top"
                   FontSize="16" >Укажите путь к файлу</TextBlock>
        <TextBox x:Name="LoadTextBox" Grid.Row="1" HorizontalAlignment="Center" Width="200"
                 Height="20"  />
        <Button Grid.Row="1" Height="20" Width="45" Content="ОК"
                HorizontalAlignment="Right" Margin="10,10,10,10"  />
    </Grid>
</Window>
